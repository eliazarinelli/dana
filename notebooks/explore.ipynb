    "from ipywidgets import *\n",
    "from IPython.display import display\n",
    "\n",
    "from context import dana\n",
    "store = dana.dana.Dana()\n",
    "\n",
    "text = widgets.Text()\n",
    "display(text)\n",
    "def h_submit(sender):\n",
    "    a = store.count_metaorders(text.value)\n",
    "    print(text.value + ': ' + str(a))\n",
    "text.on_submit(h_submit)\n"
